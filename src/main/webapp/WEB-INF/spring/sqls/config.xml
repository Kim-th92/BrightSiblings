		<typeAlias type="com.bs.dabom.model.dto.Dailyfoodrecord_Dto" alias="dailyfoodDto"/>
		<typeAlias type="com.bs.dabom.model.dto.Calendar_Dto" alias="calendarDto"/>
	</typeAliases>
	
<!-- mapper -->
	<mappers>
		<mapper resource = "/mybatis/food-mapper.xml"/>
		<mapper resource = "/mybatis/member-mapper.xml"/>
		<mapper resource = "/mybatis/friends-mapper.xml"/>
		<mapper resource = "/mybatis/board-mapper.xml"/>
		<mapper resource = "/mybatis/donation-mapper.xml"/>
		<mapper resource = "/mybatis/chat-mapper.xml"/>
		<mapper resource = "/mybatis/news-mapper.xml"/>
		<mapper resource = "/mybatis/exercise-mapper.xml"/>
		<mapper resource="/mybatis/boardres-mapper.xml" />
		<mapper resource ="/mybatis/addinfo-mapper.xml"/>
	</mappers>


</configuration>
